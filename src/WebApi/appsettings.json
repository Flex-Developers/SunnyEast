{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "WebApi.Services.DbSnapshot": "Debug",
      "WebApi.Services.DbSnapshot.DbSnapshotService": "Debug"
    }
  },

  "Backups": {
    "Directory": "App_Data/Backups",
    "FilePrefix": "snapshot",
    "RunMigrationsAfterRestore": false,
    "KeepLastLocalCopies": 10
  },

  "AllowedHosts": "*"
}
